{
  "name": "devdock",
  "version": "1.0.0",
  "description": "A full-stack web application for code repository management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "node build-client.js",
    "build-client": "cd client && npm run build",
    "start-full": "npm run build-client && npm start",
    "kill-ports": "taskkill /f /im node.exe 2>nul & echo Ports cleared",
    "dev-safe": "npm run kill-ports && npm run dev"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "groq-sdk": "^0.37.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.15.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "y-websocket": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
